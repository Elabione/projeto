@model projeto.ViewModel.ItemListViewModel
@{
    ViewData["Title"] = "Cardapio";
}
<div class="d-flex justify-content-center">
    <h1 class="text-white marrom rounded-pill ps-4 pe-4 pt-3 pb-3 my-3">@Model.CategoriaAtual</h1>
</div>
    <div class="container">
    <div class="row row-cols-md-2 row-cols-1">
        @foreach(var item in Model.Itens){
            <partial name="_Card" for="@item"></partial>
        }
    </div>
</div>